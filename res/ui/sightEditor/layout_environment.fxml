<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="0.0" minWidth="0.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.ruegnerlukas.wtsights.ui.sighteditor.UIEnvironment">
   <children>
      <ScrollPane fitToWidth="true" minHeight="0.0" minWidth="0.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0">
               <children>
                  <VBox spacing="5.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                     <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </padding>
                     <children>
                        <ComboBox fx:id="comboAmmo" prefWidth="1000000.0" />
                        <ChoiceBox fx:id="choiceZoomMode" prefWidth="1000000.0" />
                        <Separator prefWidth="1000000.0">
                           <padding>
                              <Insets bottom="10.0" top="10.0" />
                           </padding>
                        </Separator>
                        <CheckBox fx:id="cbShowRangefinder" mnemonicParsing="false" onAction="#onShowRangefinder" prefWidth="1000000.0" text="Show Rangefinder" />
                        <HBox>
                           <children>
                              <Label prefWidth="1000000.0" text="Rangefinder Progress:" />
                              <Label fx:id="labelValueRFProgress" alignment="CENTER_RIGHT" minWidth="50.0" prefHeight="21.0" prefWidth="50.0" text="100%" />
                           </children>
                        </HBox>
                        <Slider fx:id="sliderRangefinderProgress" blockIncrement="1.0" majorTickUnit="10.0" minorTickCount="0" prefWidth="1000000.0" showTickMarks="true" />
                        <Separator prefWidth="1000000.0">
                           <padding>
                              <Insets bottom="10.0" top="10.0" />
                           </padding>
                        </Separator>
                        <HBox>
                           <children>
                              <Label prefWidth="1000000.0" text="Range Correction:" />
                              <Label fx:id="labelValueRange" alignment="CENTER_RIGHT" minWidth="50.0" prefHeight="21.0" prefWidth="50.0" text="1000m" />
                           </children>
                        </HBox>
                        <Slider fx:id="sliderRangeCorrection" blockIncrement="1.0" majorTickUnit="1000.0" max="4000.0" minorTickCount="4" prefWidth="1000000.0" showTickLabels="true" showTickMarks="true" />
                        <Separator prefWidth="1000000.0">
                           <padding>
                              <Insets bottom="10.0" top="10.0" />
                           </padding>
                        </Separator>
                        <CheckBox fx:id="cbCrosshairLighting" mnemonicParsing="false" onAction="#onCrosshairLighting" prefWidth="1000000.0" text="Enable Crosshair Lighting" />
                        <Separator prefWidth="1000000.0">
                           <padding>
                              <Insets bottom="15.0" top="15.0" />
                           </padding>
                        </Separator>
                        <Label prefWidth="1000000.0" text="Background:" />
                        <HBox prefWidth="1000000.0">
                           <children>
                              <Button mnemonicParsing="false" onAction="#onBrowseBackground" prefWidth="1000000.0" text="Browse" />
                              <Button minWidth="35.0" mnemonicParsing="false" onAction="#onResetBackground" text="X" />
                           </children>
                        </HBox>
                        <TextField fx:id="pathBackground" editable="false" prefWidth="1000000.0" />
                        <Separator prefWidth="1000000.0">
                           <padding>
                              <Insets bottom="10.0" top="10.0" />
                           </padding>
                        </Separator>
                        <Label prefWidth="1000000.0" text="Resolution:" />
                        <ChoiceBox fx:id="choiceResolution" prefWidth="1000000.0" />
                     </children>
                  </VBox>
               </children>
            </AnchorPane>
        </content>
      </ScrollPane>
   </children>
</AnchorPane>
